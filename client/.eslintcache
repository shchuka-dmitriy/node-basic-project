t/client/src/reducers/index.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/sagas/rootSaga.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/pages/Home/Home.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/pages/RegistrationPage/RegistrationPage.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/sagas/authSagas.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/reducers/authReducer.js",["189"],"import ACTION from '../actions/actionTypes';\n\nconst initialState = {\n    isFetching: false,\n    error: null\n};\n\n// eslint-disable-next-line import/no-anonymous-default-export\nexport default function (state = initialState, action) {\n    switch (action.type) {\n        case ACTION.AUTH_ACTION_REQUEST: {\n            return {\n                isFetching: true,\n                error: null\n            }\n        }\n        case ACTION.AUTH_ACTION_SUCCESS: {\n            return {\n                isFetching: false,\n                error: null\n            }\n        }\n        case ACTION.AUTH_ACTION_ERROR: {\n            return {\n                isFetching: false,\n                error: action.error\n            }\n        }\n        case ACTION.AUTH_ACTION_CLEAR_ERROR:{\n            return{\n                ...state,\n                error: null\n            }\n        }\n        case ACTION.AUTH_ACTION_CLEAR:{\n            return initialState;\n        }\n        default:\n            return state;\n    }\n}","/home/developer/IdeaProjects/node-basic-project/client/src/actions/actionTypes.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/actions/actionCreator.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/constants.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/components/Header/Header.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/api/rest/restController.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/components/FormInput/FormInput.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/validators/validator.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/validators/validationSchemes.js",["190"],"import * as yup from 'yup';\n\nexport default {\n    LoginSchema: yup.object().shape({\n        email: yup.string().email('check email').required('Email is required'),\n        password: yup.string().test('test-password','min 6 symbols',value => (value && value.trim().length>=6)).required('Password is required')\n    }),\n    RegistrationSchema: yup.object().shape({\n        email: yup.string().email('check email').required('Email is required'),\n        password: yup.string().test('test-password','min 6 symbols',value => (value && value.trim().length>=6)).required('Password is required'),\n        firstName: yup.string().test('test-firstName','required',value => (value && value.trim().length>=1)).required('First Name is required'),\n        lastName: yup.string().test('test-lastName','required',value => (value && value.trim().length>=1)).required('Last Name is required')\n    }),\n    ProductSchema: yup.object().shape({\n        weight: yup.number().min(1,'min weight is 1').required('Weight field is required'),\n        color: yup.string().test('test-color','required',value => (value && value.trim().length>=1)).required('Color field is required'),\n        price: yup.number().min(0.1,'min price is 0.1').required('Price field is required'),\n        productType: yup.string().matches(/(phone|tablet|notebook)/).required('Product type is required'),\n        dualSim: yup.boolean().oneOf([true, false],'Need to select type of sim card'),\n        videoCard: yup.string().test('test-videoCard', value => (value && value.trim().length>=1))\n    }),\n}\n","/home/developer/IdeaProjects/node-basic-project/client/src/api/interceptor.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/components/UserForm/UserForm.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/components/Error/Error.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/pages/LoginPage/LoginPage.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/pages/ProductPage/ProductPage.js",["191","192"],"import React from 'react';\nimport {connect} from 'react-redux';\nimport styles from './ProductPage.module.sass';\nimport CONSTANTS from '../../constants';\nimport TryAgain from '../../components/TryAgain/TryAgain';\nimport { deleteProductAction, getProductsAction,\n    getProductAction, getProductByIdAction, headerRequest } from \"../../actions/actionCreator\";\nimport BackButton from \"../../components/BackButton/BackButton\";\nimport ProductInfo from \"../../components/ProductInfo/ProductInfo\";\nimport Spinner from '../../components/Spinner/Spinner';\n// import ProductForm from \"../../components/ProductForm/ProductForm\";\n\nclass ProductPage extends React.Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            isEdit: false\n        }\n    }\n\n    componentDidMount() {\n        this.getData();\n    }\n\n    /**\n     *\n     * @description Get product data from server\n     */\n    getData = () => {\n        // this.props.getUserRole();\n        const { params } = this.props.match;\n        this.props.getProduct(params.id);\n    };\n\n    /**\n     *\n     * @description Delete product\n     * @param {Class} product\n     */\n    deleteCurrentProduct = (product) => {\n        if ( window.confirm('Are you sure wish to delete this Product?') ) {\n            this.props.deleteProduct(product.currentTarget.id);\n            this.props.getProductsArr();\n            window.location.assign(CONSTANTS.HOME_URL);\n        }\n    };\n\n    /**\n     *\n     * @description Set view mode between Display and Edit product\n     */\n    changeViewMode = () => {\n        this.setState({\n            isEdit: !this.state.isEdit\n        })\n    }\n\n    /**\n     *\n     * @description Get default data about product for Edit page\n     */\n    getProductObjInfo = () => {\n        const {productName, Attribute: { price, weight, color, dualSim, videoCard } } = this.props.selectProductStore.selectProductData.product;\n        const defaultData = {};\n        const data = { productName, price, weight, color, dualSim, videoCard };\n\n        Object.keys(data).forEach((key) => {\n            if (data[key]) {\n                defaultData[key] = data[key];\n            }\n        });\n        return defaultData;\n    };\n\n    render() {\n        const {isEdit} = this.state;\n        const {productStore, selectProductStore, match: {params: {id}}, error} = this.props;\n        const {selectProductData, isFetching} = selectProductStore;\n\n        return (\n            <div>\n                {error ? <div className={styles.tryContainer}><TryAgain getData={this.getData}/></div> :\n                    (\n                        isFetching ?\n                            <div className={styles.containerSpinner}>\n                                <Spinner/>\n                            </div>\n                            :\n                            <div className={styles.mainInfoContainer}>\n                                <div className={styles.backButtonsContainer}>\n                                    <BackButton/>\n                                    {\n                                        // сделать только для авторизированных пользователей &&\n                                        <div>\n                                            <button id={id} onClick={this.deleteCurrentProduct}\n                                                    className={styles.buttonEdit}>\n                                                Delete\n                                            </button>\n                                            <button onClick={this.changeViewMode} className={styles.buttonEdit}>\n                                                {isEdit ? \"Cancel\" : \"Edit\"}\n                                            </button>\n                                        </div>\n                                    }\n                                </div>\n                                <div className={styles.mainContainerWrapper}>\n                                    {\n                                        // isEdit ? <ProductForm defaultData={this.getProductObjInfo()}\n                                        //                productId={selectProductData.product.id}\n                                        //                isEdit={isEdit}\n                                        //          />\n                                        //     :\n                                            <div className={styles.infoContainer}>\n                                                <ProductInfo productData={selectProductData} productId={id}/>\n                                            </div>\n                                    }\n                                </div>\n                            </div>\n                    )\n                }\n            </div>\n        )\n    }\n}\n\nconst mapStateToProps = ({selectProductStore, productStore}) => {\n    return {selectProductStore, productStore};\n};\n\nconst mapDispatchToProps = (dispatch) => {\n    return {\n        isUserRegistered: () => dispatch(headerRequest()),\n        getProductsArr: () => dispatch(getProductsAction()),\n        getProduct: (id) => dispatch(getProductByIdAction(id)),\n        deleteProduct: (id) => dispatch(deleteProductAction(id))\n    }\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ProductPage);","/home/developer/IdeaProjects/node-basic-project/client/src/components/NotFound/NotFound.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/reducers/getProductsReducer.js",[],"/home/developer/IdeaProjects/node-basic-project/client/src/reducers/getProductByIdReducer.js",[],"/home/